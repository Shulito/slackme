{
    "manifest_version": 2,
    "name": "SlackMe",
    "version": "1.0",
    "description": "Simulates a click on slack every 30 seconds.",
    "icons": {
        "48": "icons/slackme-48.png"
    },
    "content_scripts": [
        {
        "matches": ["*://app.slack.com/*"],
        "js": ["slackme.js"]
        }
    ]
}
